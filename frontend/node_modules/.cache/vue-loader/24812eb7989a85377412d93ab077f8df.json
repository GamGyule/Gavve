     axios.get(\"/api/banner-list\").then((res)=>{\r\n          this.$store.state.bannerData = res.data;\r\n          console.log(\"BannerLoading\");\r\n        }).catch((err)=>{\r\n          console.log(err);\r\n        })\r\n        break;\r\n    }\r\n  }\r\n\r\n  bannerAdd(){\r\n    this.$store.commit(\"ADD_BANNER\");\r\n  }\r\n\r\n  NavToggleBtn() {\r\n    this.navToggle = !this.navToggle;\r\n  }\r\n  \r\n}\r\n</script>\r\n\r\n<style\r\n  src=\"../../../../../src/main/resources/css/Admin.styl\"\r\n  lang=\"stylus\"\r\n></style>\r\n"]}]}